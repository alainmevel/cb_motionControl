				<body>
					<ST>(* toto fait du velo *)

if ((masterPosition &gt;=0.0) and (masterPosition &lt; endPhase1 )) then phaseCode := 1; end_if;
if ((masterPosition &gt;= endPhase1 ) and (masterPosition &lt;endPhase2)) then phaseCode := 2; end_if;
if ((masterPosition &gt;= endPhase2 ) and (masterPosition &lt;endPhase3)) then phaseCode := 3; end_if;
if ((masterPosition &gt;= endPhase3 ) and (masterPosition &lt;endPhase4)) then phaseCode := 4; end_if;
if ((masterPosition &gt;= endPhase4 ) and (masterPosition &lt;360.0)) then phaseCode := 5; end_if;

if ((phaseCode = 2) and (not presConvDw or not weightOk)) then phaseCode := 0; end_if;
if ((phaseCode = 3) and presConvUp) then phaseCode := 0; end_if;
if (not run) then  phaseCode := 0; end_if;

CASE phaseCode OF
&#9;0: &#9;MoveVelCmd := false;


&#9;1 : &#9;MoveVelCmd := true ;  (* Approach to take the product *)
&#9;&#9;MoveSpeed := speedPhase1 * globalSpeedFactor /100.0 ;
&#9;&#9;gripSyringes := false;
&#9;&#9;
&#9;2 :&#9;MoveVelCmd := true ;&#9;(* take the product slowly *)
&#9;&#9;MoveSpeed := speedPhase2 * globalSpeedFactor /100.0 ;
&#9;&#9;gripSyringes := true;

&#9;3 :&#9;MoveVelCmd := true ;&#9;(* Go the the second conveyor *)
&#9;&#9;MoveSpeed := speedPhase3 * globalSpeedFactor /100.0 ;
&#9;&#9;gripSyringes := true;

&#9;4 :&#9;MoveVelCmd := true ;&#9;(* put down the product *)
&#9;&#9;MoveSpeed := speedPhase4 * globalSpeedFactor /100.0 ; &#9;
&#9;&#9;gripSyringes := true;
&#9;
&#9;5 :&#9;MoveVelCmd := true ;&#9;(* go back *)
&#9;&#9;MoveSpeed := speedPhase4 * globalSpeedFactor /100.0 ; &#9;
&#9;&#9;gripSyringes := false;

ELSE &#9;
&#9;MoveVelCmd := false ;
&#9;
END_CASE;
</ST>
